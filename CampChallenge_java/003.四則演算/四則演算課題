            final int num2 = 2;
            int tasu = num + num2;
            int hiku = num - num2;
            int kake = num * num2;
            int waru = num / num2;
            int jyouyo = num % num2;
            out.print(tasu);
            out.print(hiku);
            out.print(kake);
            out.print(waru);
            out.print(jyouyo);
